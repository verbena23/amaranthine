<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://openbox.org/                 file:///usr/share/openbox/menu.xsd">
	<menu id="root-menu" label="Openbox 3">
		<item label="Run..">
			<action name="Execute">
				<execute>gmrun</execute>
			</action>
		</item>
		<item label="Terminal">
			<action name="Execute">
				<execute>x-terminal-emulator</execute>
			</action>
		</item>
		<item label="Web browser">
			<action name="Execute">
				<execute>palemoon</execute>
			</action>
		</item>
		<!-- This requires the presence of the 'menu' package to work -->
		<item label="File Browser">
			<action name="Execute">
				<execute>pcmanfm</execute>
			</action>
		</item>
		<item label="Text Editor">
			<action name="Execute">
				<execute>gedit</execute>
			</action>
		</item>
		<item label="Code Editor">
			<action name="Execute">
				<execute>geany</execute>
			</action>
		</item>
		<separator/>
		<menu id="/Debian"/>
		<item label="KeePassXC">
			<action name="Execute">
				<execute>keepassxc</execute>
			</action>
		</item>
		<item label="Passwords">
			<action name="Execute">
				<execute>keepassx</execute>
			</action>
		</item>
		<separator/>
		<menu id="client-list-menu"/>
		<separator/>
		<item label="Obmenu">
			<action name="Execute">
				<execute>obmenu</execute>
			</action>
		</item>
		<item label="ObConf">
			<action name="Execute">
				<execute>obconf</execute>
			</action>
		</item>
		<separator/>
		<item label="Open Box config">
			<action name="Execute">
				<execute>geany ~/.config/openbox/rc.xml</execute>
			</action>
		</item>
		<item label="Tint2 config">
			<action name="Execute">
				<execute>geany ~/.config/tint2/tint2rc</execute>
			</action>
		</item>
		<separator/>
		<item label="Reconfigure">
			<action name="Reconfigure"/>
		</item>
		<item label="Restart">
			<action name="Restart"/>
		</item>
		<separator/>
		<item label="Exit">
			<action name="Exit"/>
		</item>
	</menu>
</openbox_menu>
